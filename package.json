{
  "name": "ts-find-unused",
  "version": "1.0.0",
  "description": "CLI tool to find unused code in TypeScript projects",
  "scripts": {
    "test": "jest",
    "dev": "tsc -w -p tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "ts-find-unused": "bin/ts-find-unused.js"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeksco/ts-find-unused.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aeksco/ts-find-unused/issues"
  },
  "homepage": "https://github.com/aeksco/ts-find-unused#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^16.10.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^8.2.0",
    "ts-morph": "^12.0.0"
  }
}
